<div id="Cat">
  <img src="http://cs614717.vk.me/v614717486/5cf0/2dHRpySOW8s.jpg" >
</div>

<div id="ShowUser">
<p>
    <b>
      <%= @userWelcome %>
    </b>
</p>
<br>
<b>Skills:</b>
<br>
<%if @skillsCurrent.count == 0 %>
  Haven't any skill
<% else %>
  <% @skillsCurrent.each do |sc| %>
    <% SkillsTerc.where(id: sc.id_skills).each do |skill| %>
      <q><%= skill.name %></q>
    <% end %>
  <% end %>
<% end %>
</div>



<div id="ShowUsers">
<% @usersAll.each do |uAll| %>
  <p>
    <a><%= uAll %></a><br>
    <% @step = UskillsTerc.where(id_users: uAll.id).select(:id_skills) %>
    <% SkillsTerc.where(id: @step).each do |showSkillsUsers| %>
      <q><%= showSkillsUsers.name %></q>
    <% end %>
  </p>
<% end %>
<br>
</div>


